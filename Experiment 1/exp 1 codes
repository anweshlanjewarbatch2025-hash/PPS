1.1.1
radius = float (input())
area = 3.14 * radius * radius
print(f"{area:.4f}")


1.1.2
length = float(input())
width = float(input())
area= length*width
print(f"{area:.2f}")


1.1.3
side_length = int(input())
area = side_length * side_length
print(area)


1.1.4
base = float(input())
height = float(input())
area = 0.5 * base * height
print(f"{area:.2f}")


